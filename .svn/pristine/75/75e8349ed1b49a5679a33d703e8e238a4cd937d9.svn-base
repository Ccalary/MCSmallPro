<cover-view class='flex flex-column flex-row-between height-percent-100'>
  <cover-view class='flex'>
    <cover-view class='zhubo-info'>
      <cover-view>
        <cover-image class='zhubo-avatar' src='{{zhuboAvatar}}'></cover-image>
      </cover-view>
      <cover-view class='zhubo-name ml10'>
        <cover-view class='fs14 ellipsis'>{{zhuboName}}</cover-view>
        <cover-view>在线 {{onlineCount}}</cover-view>
      </cover-view>
      <cover-view hidden='{{isAttention}}' class='btn-attention' bindtap='attention'>关注</cover-view>
    </cover-view>
  </cover-view>
  <cover-view class='flex flex-row-end'>
    <cover-image class='image-like mb10 mr10' bindtap='like' src='http://res.o2o.com.cn:8082/img/ac491014dc054fd4a43efc70321ef6b6'></cover-image>
  </cover-view>
</cover-view>