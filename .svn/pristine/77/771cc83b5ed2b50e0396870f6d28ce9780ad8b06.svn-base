/**app.wxss**/
@import "/css/common.wxss";

view, navigator,text {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

page {
  background-color: #f3f3f4;
  color: #666;
  font-size: 26rpx;
}

.text-default {
  color: #666;
}

.text-primary {
  color: #ff5f8d;
}

button[plain].text-primary {
  color: #ff5f8d;
  border: 2rpx solid #ff5f8d;
}
